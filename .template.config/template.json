{
  "$schema": "http://json.schemastore.org/template", // https://github.com/dotnet/templating/wiki/Reference-for-template.json
  "author": "Equinor ProCoSys",
  "classifications": [ "Web/MVC" ], // Use command 'dotnet new' to see list of other classifications
  "name": "Domain Driven Design API", // Name that is displayed when running 'dotnet new' command
  "identity": "DDDAPI", // Unique name for this template
  "shortName": "dddapi", // Alternative shortname, i.e. 'dotnet new mygamestartup'
